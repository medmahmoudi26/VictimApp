{
  "key": "com.eazybi.confluence.plugins.eazybi-confluence",
  "baseUrl": "https://aod.eazybi.com",
  "name": "eazyBI for Confluence Cloud",
  "description": "eazyBI provides easy drag-and-drop creation of custom Confluence reports, charts and dashboards.",
  "vendor": {
    "name": "eazyBI",
    "url": "https://eazybi.com"
  },
  "apiVersion": 5,
  "apiMigrations": {
    "signed-install": true,
    "context-qsh": true
  },
  "lifecycle": {
    "installed": "/atlassian-connect/eazybi-confluence/installed",
    "uninstalled": "/atlassian-connect/eazybi-confluence/uninstalled",
    "enabled": "/atlassian-connect/eazybi-confluence/enabled",
    "disabled": "/atlassian-connect/eazybi-confluence/disabled"
  },
  "modules": {
    "generalPages": [
      {
        "key": "home",
        "url": "/atlassian-connect/eazybi-confluence/home?redirect_to={ac.redirect_to}&user_is_admin={condition.user_is_admin}",
        "conditions": [
          {
            "condition": "user_is_logged_in"
          }
        ],
        "name": {
          "value": "eazyBI"
        },
        "icon": {
          "url": "/images/eazybi-icon-white.svg"
        },
        "location": "system.header/left"
      }
    ],
    "postInstallPage": {
      "key": "getting-started",
      "url": "/atlassian-connect/eazybi-confluence/getting_started?user_is_admin={condition.user_is_admin}",
      "name": {
        "value": "Get started"
      }
    },
    "dynamicContentMacros": [
      {
        "key": "confluence-report-gadget",
        "name": {
          "value": "eazyBI for Confluence report"
        },
        "description": {
          "value": "Embed selected eazyBI for Confluence report."
        },
        "renderModes": {
          "default": {
            "url": "/atlassian-connect/eazybi-confluence/confluence_macros/report.png?params={params}&user_is_admin={condition.user_is_admin}"
          }
        },
        "url": "/atlassian-connect/eazybi-confluence/confluence_macros/report?params={params}&user_is_admin={condition.user_is_admin}",
        "editor": {
          "insertTitle": {
            "value": "Insert eazyBI for Confluence report"
          },
          "editTitle": {
            "value": "Edit eazyBI for Confluence report"
          },
          "url": "/atlassian-connect/eazybi-confluence/confluence_macros/report?editor=true&user_is_admin={condition.user_is_admin}",
          "width": "90%"
        },
        "parameters": [
          {
            "identifier": "params",
            "name": {
              "value": "Params"
            },
            "type": "string",
            "required": true
          }
        ],
        "categories": [
          "external-content",
          "reporting"
        ],
        "icon": {
          "url": "/images/eazybi_gadget_square.png"
        },
        "imagePlaceholder": {
          "width": 300,
          "height": 30,
          "url": "/atlassian-connect/eazybi-confluence/confluence_macros/image_placeholder"
        },
        "width": "100%"
      },
      {
        "key": "confluence-dashboard-gadget",
        "name": {
          "value": "eazyBI for Confluence dashboard"
        },
        "description": {
          "value": "Embed selected eazyBI for Confluence dashboard."
        },
        "renderModes": {
          "default": {
            "url": "/atlassian-connect/eazybi-confluence/confluence_macros/dashboard.png?params={params}&user_is_admin={condition.user_is_admin}"
          }
        },
        "url": "/atlassian-connect/eazybi-confluence/confluence_macros/dashboard?params={params}&user_is_admin={condition.user_is_admin}",
        "editor": {
          "insertTitle": {
            "value": "Insert eazyBI for Confluence dashboard"
          },
          "editTitle": {
            "value": "Edit eazyBI for Confluence dashboard"
          },
          "url": "/atlassian-connect/eazybi-confluence/confluence_macros/dashboard?editor=true&user_is_admin={condition.user_is_admin}",
          "width": "90%"
        },
        "parameters": [
          {
            "identifier": "params",
            "name": {
              "value": "Params"
            },
            "type": "string",
            "required": true
          }
        ],
        "categories": [
          "external-content",
          "reporting"
        ],
        "icon": {
          "url": "/images/eazybi_gadget_square.png"
        },
        "imagePlaceholder": {
          "width": 300,
          "height": 30,
          "url": "/atlassian-connect/eazybi-confluence/confluence_macros/image_placeholder"
        },
        "width": "100%"
      },
      {
        "key": "jira-report-gadget",
        "name": {
          "value": "eazyBI for Jira report"
        },
        "description": {
          "value": "Embed selected eazyBI for Jira report."
        },
        "renderModes": {
          "default": {
            "url": "/atlassian-connect/eazybi-confluence/confluence_macros/report.png?params={params}&base_plugin=eazybi-jira&user_is_admin={condition.user_is_admin}"
          }
        },
        "url": "/atlassian-connect/eazybi-confluence/confluence_macros/report?params={params}&base_plugin=eazybi-jira&user_is_admin={condition.user_is_admin}",
        "editor": {
          "insertTitle": {
            "value": "Insert eazyBI for Jira report"
          },
          "editTitle": {
            "value": "Edit eazyBI for Jira report"
          },
          "url": "/atlassian-connect/eazybi-confluence/confluence_macros/report?editor=true&base_plugin=eazybi-jira&user_is_admin={condition.user_is_admin}",
          "width": "90%"
        },
        "parameters": [
          {
            "identifier": "params",
            "name": {
              "value": "Params"
            },
            "type": "string",
            "required": true
          }
        ],
        "categories": [
          "external-content",
          "reporting"
        ],
        "icon": {
          "url": "/images/eazybi_gadget_square.png"
        },
        "imagePlaceholder": {
          "width": 300,
          "height": 30,
          "url": "/atlassian-connect/eazybi-confluence/confluence_macros/image_placeholder?base_plugin=eazybi-jira"
        },
        "width": "100%"
      },
      {
        "key": "jira-dashboard-gadget",
        "name": {
          "value": "eazyBI for Jira dashboard"
        },
        "description": {
          "value": "Embed selected eazyBI for Jira dashboard."
        },
        "renderModes": {
          "default": {
            "url": "/atlassian-connect/eazybi-confluence/confluence_macros/dashboard.png?params={params}&base_plugin=eazybi-jira&user_is_admin={condition.user_is_admin}"
          }
        },
        "url": "/atlassian-connect/eazybi-confluence/confluence_macros/dashboard?params={params}&base_plugin=eazybi-jira&user_is_admin={condition.user_is_admin}",
        "editor": {
          "insertTitle": {
            "value": "Insert eazyBI for Jira dashboard"
          },
          "editTitle": {
            "value": "Edit eazyBI for Jira dashboard"
          },
          "url": "/atlassian-connect/eazybi-confluence/confluence_macros/dashboard?editor=true&base_plugin=eazybi-jira&user_is_admin={condition.user_is_admin}",
          "width": "90%"
        },
        "parameters": [
          {
            "identifier": "params",
            "name": {
              "value": "Params"
            },
            "type": "string",
            "required": true
          }
        ],
        "categories": [
          "external-content",
          "reporting"
        ],
        "icon": {
          "url": "/images/eazybi_gadget_square.png"
        },
        "imagePlaceholder": {
          "width": 300,
          "height": 30,
          "url": "/atlassian-connect/eazybi-confluence/confluence_macros/image_placeholder?base_plugin=eazybi-jira"
        },
        "width": "100%"
      }
    ],
